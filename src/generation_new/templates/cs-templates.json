[
  {
    "id": "cs-source-file",
    "name": "C# File",
    "body": [
      "$cs-using",
      "",
      "$cs-namespace"
    ]
  },
  {
    "id": "cs-using",
    "name": "C# Using Declaration",
    "repeatable": true,
    "body": [
      "using $0;"
    ]
  },
  {
    "id": "cs-namespace",
    "name": "C# Namespace",
    "repeatable": true,
    "body": [
      "namespace $0",
      "{",
      "    $cs-class",
      "}"
    ]
  },
  {
    "id": "cs-class",
    "name": "C# Class",
    "joiner": "",
    "repeatable": true,
    "body": [
      "$0 class $1$cs-parent-class",
      "{",
      "    $cs-class-body",
      "}"
    ]
  },
  {
    "id": "cs-class-body",
    "name": "C# Class Body",
    "repeatable": true,
    "body":[
      "$cs-class",
      "$cs-property",
      "$cs-method-extern",
      "$cs-method"
    ]
  },
  {
    "id": "cs-parent-class",
    "name": "C# Parent Class",
    "body": [ " : $0" ]
  },
  {
    "id": "cs-property",
    "name": "C# Property",
    "repeatable": true,
    "body": [
      "$0 $1 $2",
      "{",
      "    $cs-property-get",
      "    $cs-property-set",
      "}"
    ]
  },
  {
    "id": "cs-property-get",
    "name": "C# Property Getter",
    "body": [
      "get",
      "{",
      "    $0",
      "}"
    ]
  },
  {
    "id": "cs-property-set",
    "name": "C# Property Setter",
    "body": [
      "get",
      "{",
      "    $0",
      "}"
    ]
  },
  {
    "id": "cs-method-extern",
    "name": "C# Extern Method",
    "joiner": "",
    "repeatable": true,
    "body": [
      "[MethodImpl(MethodImplOptions.InternalCall)]",
      "$0 extern static $1 $2($cs-method-arg);",
      ""
    ]
  },
  {
    "id": "cs-method",
    "name": "C# Method",
    "joiner": "",
    "repeatable": true,
    "body": [
      "$0 $1 $2($cs-method-arg)",
      "{",
      "    $cs-method-body",
      "}"
    ]
  },
  {
    "id": "cs-method-arg",
    "name": "C# Method Argument",
    "joiner": ", ",
    "body": [
      "$0 $1"
    ]
  },
  {
    "id": "cs-method-body",
    "name": "C# Method Body",
    "repeatable": true,
    "body": [ "$0" ]
  }
]